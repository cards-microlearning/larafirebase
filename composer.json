{
    "name": "seekoya/larafirebase",
    "description": "Laravel Firebase Cloud Messaging. (Seekoya Fork)",
    "homepage": "https://github.com/gg-innovative/larafirebase",
    "license": "MIT",
    "authors": [
        {
            "name": "Gentrit Abazi",
            "email": "gentritabazi01@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Seekoya\\Larafirebase\\": "src/"
        },
        "files" : [
            "src/Helpers/All.php"
        ]
    },
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/notifications": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "google/apiclient": "^2.12"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Seekoya\\Larafirebase\\Providers\\LarafirebaseServiceProvider"
            ]
        }
    }
}
